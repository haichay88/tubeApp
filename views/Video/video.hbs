<div class="site-output">
    <div id="all-output" class="col-md-12">
        <div class="row">
            <div class="col-md-8 col-sm-12 col-xs-12 player-holder">
                <div class="wrapper">
                    <iframe id="player" class="player" width="100%" height="100%" allow="autoplay; encrypted-media" src="https://www.youtube.com/embed/{{video.videoId}}?autoplay=1"
                        frameborder="0" allowfullscreen="1"></iframe>
                </div>
                <div class="video_info_actions clearfix" itemscope="" itemtype="http://schema.org/VideoObject">
                    <div class="clearfix detail-bg">
                        <meta itemprop="uploadDate" content="{{video.publishDated}}">
                        <meta itemprop="thumbnailUrl" content={{video.imgUrl}}>
                        <meta itemprop="url" content="{{video.url}}">
                        <meta itemprop="duration" content="{{video.duration}}">
                        <meta itemprop="name" content="{{video.title}}">
                        <div class="clearfix title-area">
                            <h1 class="">{{video.title}}</h1>
                        </div>
                        {{!-- <div class="fb-like" data-href="{{video.url}}" data-layout="standard" data-action="like" data-size="small" data-show-faces="true"
                            data-share="true"></div> --}}

                        <div class="user-likes-count clearfix hidden-xs hidden-sm">
                            <div class="user-block col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <div class="user-info">
                                    <a itemscope="" itemprop="publisher" itemtype="http://schema.org/Organization" href="/Channel/{{video.channelId}}/{{video.channelTitleConverted}}.html"
                                        id="ava_img" class="avatar" style="display:inline-block;">
                                        <meta itemprop="url" content="/Channel/{{video.channelId}}/{{video.channelTitleConverted}}.html">
                                        <meta itemprop="logo" content="{{channelInfo.imgUrl}}">
                                        <meta itemprop="name" content="{{video.channelTitle}}">
                                        <img itemprop="image" src="{{channelInfo.imgUrl}}" alt="Complex"> </a>
                                    <div class="sub-title">
                                        <strong class="user_title" itemprop="author" itemscope="" itemtype="http://schema.org/Person">
                                            <a href="/Channel/{{video.channelId}}/{{video.channelTitleConverted}}.html">{{video.channelTitle}}</a>
                                        </strong>
                                        <div id="buttid" class="clearfix btn-holder ">
                                            <a id="subscribeUser" class="btn btn-subscribe btn-primary" href="#" data-toggle="tooltip" title="" data-original-title="Subscribe">
                                                <span>
                                                    <i class="fa fa-youtube-play" aria-hidden="true"></i>Subscribe</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix like-rating-block col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <div class="clearfix" itemscope="" itemtype="http://schema.org/Rating">
                                    <span class="views-count" itemprop="ratingValue">Views: {{video.viewCount}}</span>
                                    <meta itemprop="ratingCount" content="{{video.viewCount}}"> </div>
                                <div class="clearfix like-rating" id="rating_container">
                                    <div class="rating clearfix">
                                        <div class="clearfix rating-holder">
                                            <div class="ratting-progress clearfix">
                                                <span style="width:94%;"></span>
                                            </div>
                                            <div class="like-dislike">
                                                <a class="btn-like">
                                                    <i></i>
                                                    <span id="l_num">&nbsp;{{video.likeCount}}</span>
                                                </a>
                                            </div>
                                            <div class="like-dislike">
                                                <a class="btn-dislike">
                                                    <i></i>
                                                    <span id="disl_num">&nbsp;{{video.dislikeCount}}</span>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="descr_list" class="tune-tabs clearfix detail-bg hidden-xs hidden-sm">
                        <ul class="clearfix nav nav-tabs">
                            <li class="active">
                                <a href="#desctab" data-toggle="tab">Description</a>
                            </li>
                            <li>
                                <a id="soc_share" href="#" data-toggle="tab">
                                    <span class="fa fa-share"></span>&nbsp;Share</a>
                            </li>
                        </ul>
                        <div class="tab-content clearfix">
                            <div class="tab-pane fade in clearfix active" id="cb-item-share">
                                <i id="closesh" class="fa fa-times fa-lg"></i>
                                <div class="addthis_inline_share_toolbox"></div>
                                <div id="share_form_div" class="action_box share_form clearfix">
                                    <div class="action_box_title"></div>
                                    <div class="form-group">
                                        <label for="link_video">Link</label>
                                        <input type="text" name="link_video" id="link_video" value="{{video.url}}" class="form-control"> </div>

                                </div>
                                <!-- Go to www.addthis.com/dashboard to customize your tools --><div class="addthis_inline_share_toolbox_y0el"></div>
                            </div>
                            <div class="tab-pane fade in active clearfix panel-collapse collapse" id="moretab">
                                <div class="panel-body-content">
                                    <div class="clearfix watch-vid-details">
                                        <div style="white-space: pre-wrap;" itemprop="description">
                                            {{video.description}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                        </div>
                    </div>
                    <div class="clearfix detail-bg hidden-xs hidden-sm" id="comments-block">
                        <div id="reply" class="clearfix">
                            <div class="pad-bottom">
                                <div class="addComment clearfix">
                                    <h2 class="com_count">Comment</h2>
                                </div>
                                <div id="userCommentsList" class="">
                                    {{!-- <div class="fb-comments" data-href="{{video.url}}" data-numposts="5" data-width="100%"></div> --}}
                                    <div class="partialContents" data-url="/comment/{{video.videoId}}">
                                        Loading ...
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix detail-bg" id="ads_bot_detail" style="border-bottom: none !important;padding-top: 5px; text-align: center;">
                    </div>
                </div>
            </div>
            <div class="partialContents" data-url="/videorelated/{{video.videoId}}">
                Loading ...
            </div>

           
        </div>
    </div>
</div>