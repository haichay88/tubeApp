<!DOCTYPE html>
<html itemscope itemtype="http://schema.org/WebPage" lang="{{{__ "LangCode"}}}">

<head>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Content-Language" content="{{{__ "LangCode2"}}}">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{meta.title}}</title>
    <meta name="robots" content="index,follow" />

    <!-- Jetpack Open Graph Tags -->
    <meta property="og:type" content="article" />
    <meta property="og:title" content="{{meta.title}}" />
    <meta property="og:url" content="{{meta.url}}" />
    <meta property="og:description" content="{{meta.tags}}" />

    <meta property="article:published_time" content="{{meta.publishDated}}" />
    <meta property="article:modified_time" content="{{meta.publishDated}}" />

    <meta property="og:site_name" content="{{meta.domain}}" />
    <meta property="og:image" content="{{meta.imgUrl}}" />

    <meta property="og:image:width" content="{{meta.width}}" />
    <meta property="og:image:height" content="{{meta.height}}" />

    <meta property="og:locale" content="{{{__ "LangCode2"}}}" />
    <meta name="twitter:site" content="{{meta.domain}}" />
    <meta name="twitter:text:title" content="{{meta.title}}" />
    <meta name="twitter:image" content="{{meta.imgUrl}}" />
    <meta name="twitter:card" content="summary" />
    <!-- the rich data-->
    <meta itemprop="name" content="{{meta.domain}}" />
    <link rel="alternate" href="https//{{meta.domain}}/" hreflang="x-default" />
    <link rel="canonical" href="{{meta.url}}">
    <link rel="icon" href="/images/thichxemhai.png" />
    <!-- Meta Tags -->


    <meta name="description" content="{{meta.tags}}">
    <link href="/stylesheets/bootstrap.css" rel="stylesheet" />
   
    <link href="/stylesheets/style.css" rel="stylesheet" />
    <link href="/stylesheets/fontcss.css" rel="stylesheet" />
    <link href="/stylesheets/flag.css" rel="stylesheet" />
    <link href="/stylesheets/font-awesome.min.css" rel="stylesheet" />
     <link href="/stylesheets/Site.css" rel="stylesheet" />



</head>

<body>


    {{>headerTop}}
     {{{body}}} 

{{>footer}}

     {{>menuleft}} 
     {{>regionCode}}
     {{>language}}




    <script src="/javascripts/jquery-1.10.2.js"></script>
    <script src="/javascripts/bootstrap.js"></script>
    <script src="/javascripts/custom.js"></script>
    <script>
        $(document).ready(function () {
            $(".side-opener").on({
                click: function (n) {
                    n.preventDefault();
                    $("body").toggleClass("sideactive")
                }
            });
        });
    </script>
    <div id="fb-root"></div>
    <script async>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = 'https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.11&appId=1073352086130252';
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));</script>


